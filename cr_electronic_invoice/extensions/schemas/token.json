{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Token Hacienda Validador",
  "required": [
    "refresh_token",
    "access_token",
    "expires_in",
    "refresh_expires_in",
    "id_token"
  ],
  "properties": {
    "token_type": {
      "type": "string",
      "pattern": "^(.*)$"
    },
    "refresh_token": {
      "type": "string",
      "pattern": "^(.*)$"
    },
    "not-before-policy": {
       "type": "integer",
       "default": 0,
       "examples": [
         0
       ]
     },
     "session_state": {
       "type": "string",
       "default": "",
       "pattern": "^(.*)$"
     },
     "access_token": {
       "type": "string",
       "default": "",
       "pattern": "^(.*)$"
     },
     "expires_in": {
       "type": "integer",
       "default": 0,
       "examples": [
         300
       ]
     },
     "refresh_expires_in": {
       "type": "integer",
       "default": 0,
       "examples": [
         1200
       ]
     },
     "id_token": {
       "type": "string",
       "default": "",
       "pattern": "^(.*)$"
     }
   }
}


