<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- TODO-->
        <!-- Secuencias de MR para asignar a la primera compañía.  Si hay más compañías, debemos generar las secuencias-->
        <function model="res.company" name="try_create_confirmation_sequeces">
          <value model="res.company" search="[]"/>
        </function>
        <record id="seq_electronic_invoice" model="ir.sequence">
            <field name="name">Secuencia de Factura Electrónica</field>
            <field name="code">sequece.FE</field>
            <field name="prefix"></field>
            <field name="padding">10</field>
        </record>
        <record id="seq_electronic_debit_note" model="ir.sequence">
            <field name="name">Secuencia de Nota Débito Electrónica</field>
            <field name="code">sequece.ND</field>
            <field name="prefix"></field>
            <field name="padding">10</field>
        </record>
        <record id="seq_electronic_credit_note" model="ir.sequence">
            <field name="name">Secuencia de Nota Crédito Electrónica</field>
            <field name="code">sequece.NC</field>
            <field name="prefix"></field>
            <field name="padding">10</field>
        </record>
        <record id="seq_electronic_ticket" model="ir.sequence">
            <field name="name">Secuencia de Tiquete Electrónico</field>
            <field name="code">sequece.TE</field>
            <field name="prefix"></field>
            <field name="padding">10</field>
        </record>
        <record id="seq_electronic_exportation_invoice" model="ir.sequence">
            <field name="name">Secuencia de Factura Electrónica de Exportación</field>
            <field name="code">sequece.FEE</field>
            <field name="prefix"></field>
            <field name="padding">10</field>
        </record>
        <record id="seq_electronic_supplier_invoice" model="ir.sequence">
            <field name="name">Secuencia de Factura Electrónica de Compra</field>
            <field name="code">sequece.FEC</field>
            <field name="prefix"></field>
            <field name="padding">10</field>
        </record>
    </data>
</odoo>
